<% if @diagnosis && @patient_diagnosis.persisted? %>
  $('#patient-diagnoses').append("<tr><td class='cell-wrap'>" + "<%= escape_javascript(@patient_diagnosis.diagnosis.description) %>" + "</td>\
  <td>" + "<%= @patient_diagnosis.diagnosis.code %>" + "</td>\
    <td>" + "<%= @patient_diagnosis.diagnosis.code_type %>" + "</td>\
    <td>" + "<%= @patient_diagnosis.created_at.to_s(:date) %>" + "</td>\
    <td><form action=" + "<%= user_diagnosis_url(@user, @patient_diagnosis) %>" + " class='button_to' method='post'><div><input name='_method' type='hidden' value='delete'><input class='delete-button' type='submit' value='Delete'><input name='authenticity_token' type='hidden' value=" + "<%= form_authenticity_token %>" + "></div></form>" + "</td>\
  </tr>");
<% end %>

var flashError = "<%= flash[:errors].join('<br>').html_safe if flash[:errors] %>";

