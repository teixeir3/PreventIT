<% if current_user.is_doctor %>
  <%= render "doctors/sub_bar", doctor: current_user%>
<% else %>
  <%= render "users/sub_bar", user: current_user%>
<% end %>

<main class="group">

  <div class="side-content">
    <div class="title">
      <h2>Diagnoses:</h2>
    </div>

      <p><%= link_to "Add Diagnosis", new_diagnosis_url %></p>

  </div>

  <div class="sub-content">
    <% @diagnoses.each do |diagnosis| %>
      <%= diagnosis.code %>
    <% end %>
  </div>
</main>